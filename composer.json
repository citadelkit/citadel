{
    "name": "citadelkit/citadel",
    "description": "Citadel is laravel fullstack wrapper. Inspired by Laravel Filament",
    "type": "library",
    "require": {
        "laravel/framework": "10|11|12",
        "laravel/tinker": "^2.8",
        "fakerphp/faker": "^1.24"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Citadelkit\\Citadel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Zun",
            "email": "malikal.j4@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Citadel\\Providers\\CitadelServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
